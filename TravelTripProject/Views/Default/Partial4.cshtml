@using TravelTripProject.Models.Entities
@model List<Blog>
<div class="padding1">
    <h4>En Beğendiğim Yerler</h4>
    <div class="box-left">
        @foreach (var item in Model)
        {
            <div class="bot-img">
                <img src="@item.BlogWrite" alt="" />
            </div>
            <div class="bot-desc">
                <p>@item.Description.Substring(0,80)</p>
            </div>

            <div class="clear"></div>
        }

    </div>
@Html.Action("Partial5", "Default")
</div>
