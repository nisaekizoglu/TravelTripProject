@using TravelTripProject.Models.Entities
@model List<Blog>
<div class="padding">
    <h4>En Popüler 10 Blog</h4>
    <ul class="list list-pad">
        @{int i = 0; }
        @foreach (var item in Model)
        {
            <li class="list-bg"><span>@{i++;}@i.</span><a href="/Blog/BlogDetay/@item.BlogId">@item.Title</a></li>
        }
    </ul>
    <a href="/Blog/Index/" class="link1">Tüm blogları gör</a>
</div>
